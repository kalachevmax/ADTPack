{
  "name": "semantic.ds",

  "buildDir": "bin",

  "compilerOptions": {
    "compilationLevel": "SIMPLE_OPTIMIZATIONS",
    "warningLevel": "VERBOSE",
    "language": "ECMASCRIPT5",
    "formatting": ""
  },

  "srcDir": "lib",
  "rootNamespace": "ds",

  "modules": {
    "semantic.ds": {
      "src": [
        "ds.js",
        "i-data-item.js",
        "i-data-set.js",
        "i-iterator.js",
        "i-queue.js",
        "list-item.js",
        "list-iterator.js",
        "list.js",
        "queue.js"
      ]
    }
  },

  "externsDir": "externs",

  "deps": {
    "semantic.utils": "https://github.com/kalchevmax/semantic.utils"
  }
}